{
  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run prisma:test:deploy",
    "test": "dotenv -e .env.test -- jest",
    "pretest:test": "npm run db:test:restart"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.3",
    "@types/jest": "^29.5.3",
    "@types/passport-jwt": "^3.0.9",
    "jest": "^29.6.4",
    "jest-cucumber": "^3.0.1",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@angular/common": "^16.2.5",
    "@angular/core": "^16.2.4",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.2.3",
    "@prisma/client": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv-cli": "^7.3.0",
    "pactum": "^3.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "supertest": "^6.3.3"
  }
}
